<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chromeeye1">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoce"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="vibedev.xbcad.chromeye.Contact"></activity>
        <activity android:name="vibedev.xbcad.chromeye.Profile" />
        <activity android:name="vibedev.xbcad.chromeye.Info" />
        <activity android:name="vibedev.xbcad.chromeye.EyeTest" />
        <activity android:name="vibedev.xbcad.chromeye.Home" />
        <activity android:name="vibedev.xbcad.chromeye.Register" />
        <activity android:name="vibedev.xbcad.chromeye.Login" />
        <activity android:name="vibedev.xbcad.chromeye.SplashScreen" />
        <activity android:name="vibedev.xbcad.chromeye.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>